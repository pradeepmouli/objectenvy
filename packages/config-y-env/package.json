{
  "name": "@objectenvy/config-y-env-legacy",
  "version": "1.0.0",
  "description": "CLI tool to generate TypeScript types and schemas from .env files with smart type inference",
  "keywords": [
    "env",
    "environment",
    "dotenv",
    "typescript",
    "types",
    "generator",
    "inference",
    "zod",
    "json-schema",
    "cli"
  ],
  "license": "MIT",
  "author": "",
  "type": "module",
  "bin": {
    "config-y-env": "./bin/config-y-env.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:native": "tsgo -p tsconfig.json",
    "dev": "tsx watch src/cli.ts",
    "check:native": "tsgo --noEmit -p tsconfig.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "oxlint src",
    "format": "oxfmt src",
    "format:check": "oxfmt --check src"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "ts-morph": "^21.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.9.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "zod": "^4.3.4"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
