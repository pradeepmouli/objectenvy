{
  "name": "objectenvy-cli",
  "version": "1.1.0",
  "description": "CLI tools for ObjectEnvy - Generate .env files from TypeScript/JSON schemas",
  "keywords": [
    "cli",
    "env",
    "environment",
    "dotenv",
    "typescript",
    "config",
    "generator"
  ],
  "license": "MIT",
  "author": "",
  "type": "module",
  "private": false,
  "bin": {
    "objectenvy": "./bin/objectenvy-cli.js"
  },
  "main": "./dist/cli.js",
  "types": "./dist/cli.d.ts",
  "module": "./dist/cli.js",
  "exports": {
    ".": {
      "import": "./dist/cli.js",
      "types": "./dist/cli.d.ts"
    }
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx src/cli.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "oxlint src",
    "format": "oxfmt src",
    "format:check": "oxfmt --check src",
    "type-check": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "objectenvy": "workspace:*",
    "ts-morph": "^25.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=18"
  }
}
