{
  "name": "objectenvy-cli",
  "version": "1.0.0",
  "description": "Unified CLI for env-y-config and config-y-env under the objectenvy namespace",
  "keywords": [
    "env",
    "environment",
    "dotenv",
    "configuration",
    "typescript",
    "schema",
    "generator",
    "cli",
    "objectenvy"
  ],
  "license": "MIT",
  "author": "",
  "type": "module",
  "bin": {
    "objectenvy-cli": "./bin/objectenvy-cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:native": "tsgo -p tsconfig.json",
    "dev": "tsx watch src/cli.ts",
    "check:native": "tsgo --noEmit -p tsconfig.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "oxlint src",
    "format": "oxfmt src",
    "format:check": "oxfmt --check src"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "ts-morph": "^21.0.1",
    "objectenvy": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.9.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
