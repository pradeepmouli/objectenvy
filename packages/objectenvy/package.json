{
  "name": "objectenvy",
  "version": "1.3.0",
  "description": "Automatically map process.env to strongly-typed, nested config objects with camelCase fields",
  "keywords": [
    "config",
    "configuration",
    "env",
    "environment",
    "typescript",
    "type-safe",
    "zod",
    "nested",
    "camelcase"
  ],
  "license": "MIT",
  "author": "",
  "type": "module",
  "private": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsgo -p tsconfig.json",
    "build:native": "tsgo -p tsconfig.json",
    "dev": "tsx watch src/index.ts",
    "check:native": "tsgo --noEmit -p tsconfig.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "oxlint src",
    "format": "oxfmt src",
    "format:check": "oxfmt --check src",
    "type-check": "tsgo --noEmit -p tsconfig.json",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.52.0",
    "@types/node": "^25.3.3",
    "commander": "^14.0.3",
    "tsx": "^4.21.0",
    "type-fest": "^5.4.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "optionalDependencies": {
    "zod": "^4.3.6"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pradeepmouli/objectenvy",
    "directory": "packages/objectenvy"
  }
}
